{
    "inherits": [
        "atmega32u4"
    ],
    "build-tags": [
        "arduino_micro"
    ],
    "ldflags": [
        "-Wl,--defsym=_bootloader_size=4096",
        "-Wl,--defsym=_stack_size=512"
    ],
    "flash-command": "avrdude -c avr109 -p atmega32u4 -b 57600 -P {port} -U flash:w:{hex}:i",
    "emulator": "simavr -m atmega32u4 -f 16000000 {}"
}
